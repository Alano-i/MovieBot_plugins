{
  "name": "aliyunpan_signin",
  "title": "阿里云盘签到",
  "author": "NaNaKo_",
  "description": "阿里云盘签到。",
  "version": "0.4",
  "requirements": [],
  "configField": [
    {
      "fieldName": "uid",
      "fieldType": "Enum",
      "label": "推送用户",
      "helperText": "选择接收推送的用户,用户管理要独立设置推送配置才生效",
      "enumValuesRef": "UserList",
      "multiValue": true
    },
    {
      "fieldName": "channel_item",
      "fieldType": "Enum",
      "label": "推送通道名称",
      "helperText": "选择想要推送的通道,逗号间隔",
      "enumValuesRef": "ChannelNameList",
      "multiValue": true
    },
    {
      "fieldName": "refreshToken",
      "fieldType": "String",
      "label": "阿里云refreshToken",
      "helperText": "阿里云盘的refreshToken，多个请用英文逗号隔开。",
      "defaultValue": ""
    },
    {
      "fieldName": "sign_task",
      "fieldType": "Bool",
      "label": "签到任务使能",
      "helperText": "",
      "defaultValue": true
    },
    {
      "fieldName": "reward_enable",
      "fieldType": "Bool",
      "label": "签到后是否自动领奖",
      "helperText": "",
      "defaultValue": false
    },
    {
      "fieldName": "xiaoya_task",
      "fieldType": "Bool",
      "label": "小雅清理任务",
      "helperText": "清理小雅转存文件夹",
      "defaultValue": false
    },
    {
      "fieldName": "xiaoya_conf",
      "fieldType": "String",
      "label": "小雅配置",
      "helperText": "格式为refreshToken:folder_id。不支持多个。",
      "defaultValue": ""
    }
  ],
  "githubUrl": "",
  "helpDocUrl": "https://github.com/htnanako/MovieBot_plugins/tree/main/aliyunpan_signin",
  "logoUrl": "/plugins/aliyunpan_signin/logo.jpg",
  "payImageUrl": "https://p.xmoviebot.com/donate/chacha_wechat.jpg"
}